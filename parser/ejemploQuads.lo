func test(int i) {
  if(i <= 0){
    print("done");
  }else{
    print(i);
    test(i - 1);
  }
}

func factorial(int x): int {
  if(x <= 1) {
    return x;
  }
  return x * factorial(x - 1);
}

func main(): int {
  int i = factorial(5), j=0;
  int arr[5];

  for(j; j<5; j=j+1) {
    arr[j] = factorial(j);
  }
  j=0;
  for(j; j<5; j=j+1) {
    print(arr[j]);
  }
}