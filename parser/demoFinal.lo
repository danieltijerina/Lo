func factorialSerial(int n): int {
    int res = n;
    n = n - 1;
    while(n>1) {
        res = res * n;
        n = n - 1;
    }
    return res;
}

func factorialRec(int n): int {
    if(n <= 1) {
        return 1;
    }
    return n * factorialRec(n - 1);
}

func main(): int {
    print(factorialSerial(10));
    print(factorialRec(10));

    return 0;
}